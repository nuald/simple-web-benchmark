{
    "name": "vibedtest",
    "targetName": "vibedtest",
    "targetType": "executable",
    "targetPath": "bin",
    "dependencies": {
        "vibe-d": "~>0.8.2-alpha.1",
        "vibe-d:tls": "*"
    },
    "subConfigurations": {
        "vibe-d:tls": "notls"
    },
    "versions": [
        "VibeManualMemoryManagement"
    ],
    "buildTypes": {
        "release": {
             "buildOptions": ["releaseMode", "inline"],
             "dflags-posix-ldc": ["-flto=thin"]
        }
    },
    "configurations": [
        {
            "name": "dmd",
            "targetPath": "build/dmd"
        },
        {
            "name": "ldc",
            "targetPath": "build/ldc"
        }
    ]
}
